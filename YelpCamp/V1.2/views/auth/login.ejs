<div class="container">
    <h2 class="text-center">Login to your account</h2>
    <div class='container_form'>
        <form action="/login" method="post">
            <div class="form-group">
              <label for="username">Your email</label>
              <input type="email" name="username" id="username" class="form-control" placeholder="Type your email" aria-describedby="Type your email">
              <small id="Type your email" class="text-muted">Write your register email </small>
            </div>
            <div class="form-group">
              <label for="password">Password</label>
              <input type="password" class="form-control" name="password" id="password" placeholder="type a passwrod">
            </div>
            <button type="submit" class="btn btn-primary btn-block">Submit</button>
        </form>
    </div >
</div>



