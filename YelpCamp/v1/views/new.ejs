<% include partials/header %>

<form action="/campsgrounds" method="post">
    <div class="form-group">
      <label for="name">Name</label>
      <input type="text" class="form-control" name="name" id="name" aria-describedby="Name of campGround" placeholder="Add your CampGround">
      <small id="Name of campGround" class="form-text text-muted">add your camp ground</small>
    </div>
    <div class="form-group">
      <label for="image">Imagen</label>
      <input type="text" class="form-control" name="image" id="image" aria-describedby="URl image" placeholder="IMG url">
      <small id="URl image" class="form-text text-muted">The url of your CampGround</small>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>

<a href="/campsgrounds">Go back</a>

<% include partials/footer %>